#include<stdio.h>
#include<math.h>
int main(){
    double a,b,c=0;
    scanf("%lf %lf",&a,&b);
    if((a+b)>c)c=a+b;
    if((a-b)>c)c=a-b;
    if((b-a)>c)c=b-a;
    if((a*b)>c)c=a*b;
    if((a/b)>c)c=a/b;
    if((b/a)>c)c=b/a;
    if(pow(a,b)>c)c=pow(a,b);
    if(pow(b,a)>c)c=pow(b,a);
    printf("%lf",c);
    return 0;
}
