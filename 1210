#include<stdio.h>
int main() {
	int a[2];
	for (int i = 0; i < 2; i++) {
		scanf("%d", &a[i]);
		switch (a[i]) {
		case 1:
			a[i] = 400;
			break;
		case 2:
			a[i] = 340;
			break;
		case 3:
			a[i] = 170;
			break;
		case 4:
			a[i] = 100;
			break;
		default:
			a[i] = 70;
			break;
		}
	}
	if (a[0] + a[1] > 500)printf("angry");
	else printf("no angry");
	return 0;
}
